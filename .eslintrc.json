{
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "plugin:node/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "func-names": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off"
    }
  }